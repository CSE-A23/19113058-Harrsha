#include <iostream>
#include <string.h>
#include <conio.h>
#include <stdio.h>
using namespace std;
int main() 
{
	char array1[100],array2[100];
	int n;
	cout<<"Enter the String \n";
	gets(array1);
	n=strlen(array1);
	for(int i=0;i<n;i++)
	{
	array2[i]=array1[i]+=(i+1);
	}
	std::cout <<"The encrypted String \n"<< array2; 
}
